  <property name="styleSheet">
   <string notr="true">/* 主窗口背景 */
QMainWindow {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #6A11CB, stop: 1 #2575FC);
}

/* 所有按钮的基础样式 */
QPushButton {
    font-size: 18px;
    font-weight: bold;
    font-family: 'Microsoft YaHei';
    border-radius: 25px;
    min-width: 50px;
    min-height: 50px;
    border: 2px solid #555;
}

/* 数字按钮 */
QPushButton[text=&quot;0&quot;],
QPushButton[text=&quot;1&quot;],
QPushButton[text=&quot;2&quot;],
QPushButton[text=&quot;3&quot;],
QPushButton[text=&quot;4&quot;],
QPushButton[text=&quot;5&quot;],
QPushButton[text=&quot;6&quot;],
QPushButton[text=&quot;7&quot;],
QPushButton[text=&quot;8&quot;],
QPushButton[text=&quot;9&quot;],
QPushButton[text=&quot;.&quot;] {
    background-color: #FFFFFF;
    color: #2C3E50;
}

/* 运算符按钮 - 红色字体 */
QPushButton[text=&quot;×&quot;],
QPushButton[text=&quot;÷&quot;],
QPushButton[text=&quot;+&quot;],
QPushButton[text=&quot;-&quot;] {
    background-color: #F8F9FA;
    color: #E74C3C;  /* 红色 */
}

/* 等号按钮 */
QPushButton[text=&quot;=&quot;] {
    background-color: #3498DB;
    color: red;
}

/* 删除和清除按钮 */
QPushButton[text=&quot;⌫&quot;],
QPushButton[text=&quot;C&quot;] {
    background-color: #E74C3C;
    color: red;
}

/* 按钮悬停效果 */
QPushButton:hover {
    background-color: #BDC3C7;
}

/* 按钮按下效果 */
QPushButton:pressed {
    background-color: #95A5A6;
}</string>
